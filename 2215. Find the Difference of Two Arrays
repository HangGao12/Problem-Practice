class Solution(object):
    def findDifference(self, nums1, nums2):
        """
        :type nums1: List[int]
        :type nums2: List[int]
        :rtype: List[List[int]]
        """
        list_nums1 = list(set(nums1) - set(nums2))
        list_nums2 = list(set(nums2) - set(nums1))
        answer = [list_nums1, list_nums2]
        return answer
        
